{
  "devDependencies": {
    "javascript-obfuscator": "^4.1.1",
    "terser": "^5.44.0"
  },
  "scripts": {
    "minify": "terser js/main.js --compress --mangle --output js/main.min.js",
    "postminify": "npx replace-in-file 'main.js' 'main.min.js' index.html",
    "build": "npm run minify && npm run postminify"
  }
}
